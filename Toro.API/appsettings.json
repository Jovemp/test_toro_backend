{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AppSettings": {
    "Jwt": {
      "Secret": "#{JWT_SECRET}#",
      "Issuer": "#{JWT_ISSUER}#",
      "Audience": "#{JWT_AUDIENCE}#",
      "Expiration": "#{JWT_EXPIRATION}#"
    }
  },
  "AllowedHosts": "*"
}

